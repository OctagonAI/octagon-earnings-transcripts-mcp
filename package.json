{
  "name": "octagon-earnings-transcripts-mcp",
  "version": "1.0.0",
  "description": "MCP server for Earnings Call Transcripts Analysis. Provides specialized AI-powered earnings call transcript analysis with over 8,000 public companies coverage and historical data dating back to 2018.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "octagon-earnings-transcripts-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "test": "echo \"No tests specified\"",
    "start": "node dist/index.js",
    "lint": "echo \"No linting configured\"",
    "format": "echo \"No formatting configured\"",
    "prepare": "npm run build",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "mcp",
    "earnings-transcripts",
    "earnings-calls",
    "financial-analysis",
    "public-companies",
    "management-commentary",
    "analyst-questions",
    "financial-guidance",
    "market-intelligence",
    "sec-filings"
  ],
  "author": "Octagon AI",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OctagonAI/octagon-earnings-transcripts-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/OctagonAI/octagon-earnings-transcripts-mcp/issues"
  },
  "homepage": "https://docs.octagonagents.com"
